{
  "name": "Pye PyCharm",
  "type": "dark",
  "colors": {
    "notebook.cellEditorBackground": "#2B2D30",
    "notebook.editorBackground": "#2B2B2B",
    "editor.background": "#2B2B2B",
    "sideBar.background": "#2B2B2B",
    "panel.background": "#2B2B2B",
    "activityBar.background": "#2B2B2B",
    "editorGroupHeader.tabsBackground": "#2B2B2B",
    "tab.inactiveBackground": "#2B2B2B",
    "tab.activeBackground": "#2B2B2B",
    "statusBar.background": "#2B2B2B",
    "titleBar.activeBackground": "#2B2B2B",
    "titleBar.inactiveBackground": "#2B2B2B",
    "titleBar.activeForeground": "#949494",
    "foreground": "#dbdbdb",
    "sideBar.foreground": "#a4a4a4",
    "activityBar.foreground": "#767676",
    "notebook.focusedCellBorder": "#0a770aeb",
    "notebook.selectedCellBorder": "#FFFF00",
    "notebook.focusedEditorBorder": "#00000000",
    "notebook.cellBorderColor": "#00000000",
    "editor.lineHighlightBorder": "#38393800",
    "editor.lineHighlightBackground": "#ffffff08",
    "editor.occurrencesHighlightBackground": "#80008033",
    "editor.occurrencesHighlightBorder": "#800080",
    "editor.wordHighlightBackground": "#c300ff1b",
    "editor.wordHighlightBorder": "#80008000",
    "notebook.cellStatusBarItemHoverBackground": "#2B2B2B",
    "notebook.cellInsertionIndicator": "#0a770aeb",
    "notebook.cellHoverBackground": "#2B2B2B",
    "editor.foreground": "#dbdbdb"
  },
  "tokenColors": [
    {
      "scope": ["string"],
      "settings": {
        "foreground": "#CE9178"
      }
    },
    {
      "scope": ["comment"],
      "settings": {
        "foreground": "#676675"
      }
    },
    {
      "scope": ["keyword", "keyword.control"],
      "settings": {
        "foreground": "#569CD6"
      }
    },
    {
      "scope": ["support.function.builtin"],
      "settings": {
        "foreground": "#fac043",
        "fontStyle": ""
      }
    },
    {
      "scope": ["constant.numeric.dec.python"],
      "settings": {
        "foreground": "#92b96b"
      }
    },
    {
      "scope": ["storage.type.function.python", "constant.language.python", "keyword.control.flow.python", "variable.parameter.function.language.special.self.python"],
      "settings": {
        "foreground": "#c88cdc"
      }
    },
    {
      "scope": ["meta.function-call.generic.python", "keyword.operator.comparison.python"],
      "settings": {
        "foreground": "#e4d314",
        "fontStyle": ""
      }
    },
    {
      "scope": ["keyword.operator.logical.python", "keyword.operator.arithmetic.python", "variable.parameter.function.language.python"],
      "settings": {
        "foreground": "#61afe1",
        "fontStyle": ""
      }
    },
    {
      "scope": ["text.html.markdown meta.tag.inline", "text.html.markdown meta.tag.block.any.html"],
      "settings": {
        "foreground": "#73fd0a"
      }
    },
    {
      "scope": ["string.quoted.docstring.multi.python"],
      "settings": {
        "foreground": "#4f8427"
      }
    },
    {
      "scope": ["text.html.markdown entity.name.tag"],
      "settings": {
        "foreground": "#00fe51"
      }
    },
    {
      "scope": ["text.html.markdown punctuation.definition.tag"],
      "settings": {
        "foreground": "#00ffd0"
      }
    },
    {
      "scope": ["text.html.markdown meta.attribute-with-value"],
      "settings": {
        "foreground": "#ffff00"
      }
    }
  ]
}